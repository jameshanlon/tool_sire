% Recursively calculate the nth Fibonacci number

func fibonacci(val n) is 
{ if n > 1 
  then return fibonacci(n-1) + fibonacci(n-2)
  else if n = 0 then return 0 
  else return 1
}

% Output: 8, 89, 987
proc main() is
{  printval(fibonacci(6))  ; printstr("\n")
;  printval(fibonacci(11)) ; printstr("\n")
;  printval(fibonacci(16)) ; printstr("\n")
}
