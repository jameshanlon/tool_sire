% Calculate nth Fibonacci number

func fibonacci(val n) is 
  var x;
  var y;
  var z;
  var i;
{ x := 0 
; y := 1
; i := n - 1
; z := x
; while(i > 0) do 
  { z := x + y
  ; x := y
  ; y := z
  ; i := i - 1
  }
; return z
}

% Output: 8, 89, 987
proc main() is
{  printval(fibonacci(6))  ; printstr("\n")
;  printval(fibonacci(11)) ; printstr("\n")
;  printval(fibonacci(16)) ; printstr("\n")
}
