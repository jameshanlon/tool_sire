val N:=10;

proc bsort(var a[len], val len) is
  var i;
  var j;
  var tmp;
{ for i:=0 to len-1 do 
    for j:=0 to len-i-2 do
      if a[j] > a[j+1] then 
      { tmp := a[j]
      ; a[j] := a[j+1]
      ; a[j+1] := tmp
      }
      else skip
}

% Ouput: 0123456789
proc main() is
  var a[N];
  var i;
{ for i:=0 to N-1 do a[i] := N-(i+1)
; bsort(a, N)
; for i:=0 to N-1 do printval(a[i])
; printstrln("")
}
